<%= simple_form_for([@book,@book.reviews.build]) do |f| %>
    <div id="raiting-form">
        <label> Raiting</label>
    </div>
    
    <%= f.input :comment ,class:'form-control'%>
    <%= f.button :submit ,class:'btn btn-primary'%>

<% end %>

<script>
    $("#raiting-form").raty({
        path: "/assets/",
        scoreName:"review[raiting]"
    });
</script>